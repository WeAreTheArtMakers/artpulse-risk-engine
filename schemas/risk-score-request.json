{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RiskScoreRequest",
  "type": "object",
  "required": ["subject_id", "features"],
  "properties": {
    "subject_id": {"type": "string"},
    "event_id": {"type": "string"},
    "features": {
      "type": "array",
      "items": {"type": "number"},
      "minItems": 1
    },
    "context": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
